{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "dev": "npm run build && node --loader ts-node/esm src/app.ts --log-error",
    "build": "npx tsc",
    "start": "npm run build && node dist/src/app.js",
    "migration:show": "npm run build && ./node_modules/.bin/typeorm migration:show -d dist/config/typeormConfig.js",
    "create:entity": "npm run build &&  ./node_modules/.bin/typeorm entity:create",
    "migration:generate": "npm run build &&  ./node_modules/.bin/typeorm migration:generate -d dist/config/typeormConfig.js -o src/migrations/",
    "migration:create": "npm run build &&  ./node_modules/.bin/typeorm migration:create",
    "migration:up": "npm run build &&  ./node_modules/.bin/typeorm migration:run -d dist/config/typeormConfig.js",
    "migration:down": "npm run build &&  ./node_modules/.bin/typeorm migration:revert -d dist/config/typeormConfig.js",
    "typeorm": "npm run build &&  ./node_modules/.bin/typeorm"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "crypto-ts": "^1.0.2",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "mysql2": "^3.14.0",
    "node-fetch": "^2.7.0",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}
